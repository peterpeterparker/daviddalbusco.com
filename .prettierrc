{
  "printWidth": 120,
  "singleQuote": true,
  "arrowParens": "always",
  "bracketSpacing": false,
  "jsxBracketSameLine": true,
  "trailingComma": "none",
  "overrides": [
    {
      "files": ["*.md"],
      "options": {
        "singleQuote": false
      }
    },
    {
      "files": ["*.scss", "*.css"],
      "options": {
        "singleQuote": false
      }
    }
  ]
}
